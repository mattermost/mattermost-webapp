{
  "name": "mattermost-redux",
  "version": "5.3.1",
  "description": "Common code (API client, Redux stores, logic, utility functions) for building a Mattermost client",
  "homepage": "https://github.com/mattermost/mattermost-redux",
  "license": "Apache-2.0",
  "repository": "github:mattermost/mattermost-redux",
  "bugs": {
    "url": "https://github.com/mattermost/mattermost-redux/issues"
  },
  "files": [
    "./index*",
    "./action_types/**/*",
    "./actions/**/*",
    "./client/**/*",
    "./constants/**/*",
    "./reducers/**/*",
    "./selectors/**/*",
    "./store/**/*",
    "./utils/**/*",
    "./types/**/*",
    "./CONTRIBUTING.txt",
    "./LICENSE.txt",
    "./NOTICE.txt",
    "./README.MD",
    "./SECURITY.MD"
  ],
  "browser": "./index.js",
  "exports": {
    ".": "./index.js",
    "./action_types": "./action_types/index.js",
    "./action_types/*": "./action_types/*.js",
    "./actions": "./actions/index.js",
    "./actions/*": "./actions/*.js",
    "./client": "./client/index.js",
    "./client/*": "./client/*.js",
    "./constants": "./constants/index.js",
    "./constants/*": "./constants/*.js",
    "./reducers": "./reducers/index.js",
    "./reducers/*": "./reducers/*.js",
    "./selectors": "./selectors/index.js",
    "./selectors/*": "./selectors/*.js",
    "./store": "./store/index.js",
    "./store/*": "./store/*.js",
    "./utils": "./utils/index.js",
    "./utils/*": "./utils/*.js",
    "./types": "./types/index.js",
    "./types/*": "./types/*.js"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "cross-env NODE_ENV=production tsc",
    "check": "eslint --ignore-path .gitignore --ignore-pattern node_modules --quiet 'src/**/*.ts' 'src/**/*.test.js'",
    "fix": "eslint --ignore-path .gitignore --ignore-pattern node_modules --quiet 'src/**/*.ts' 'src/**/*.test.js' --fix",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=2",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "npm run build",
    "dev": "tsc --outDir ${WEBAPP_DIR:-../mattermost-webapp}/node_modules/mattermost-redux",
    "dev-mobile": "tsc --outDir ${MOBILE_DIR:-../mattermost-mobile}/node_modules/mattermost-redux",
    "dev:watch": "tsc --watch --outDir ${WEBAPP_DIR:-../mattermost-webapp}/node_modules/mattermost-redux",
    "dev-mobile:watch": "tsc --watch --outDir ${MOBILE_DIR:-../mattermost-mobile}/node_modules/mattermost-redux"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "form-data": "3.0.0",
    "gfycat-sdk": "1.4.18",
    "isomorphic-fetch": "2.2.1",
    "moment-timezone": "0.5.31",
    "redux": "4.0.5",
    "redux-action-buffer": "1.2.0",
    "redux-offline": "git+https://github.com/enahum/redux-offline.git#885024de96b6ec73650c340c8928066585c413df",
    "redux-persist": "4.9.1",
    "redux-persist-node-storage": "2.0.0",
    "redux-thunk": "2.3.0",
    "remote-redux-devtools": "0.5.16",
    "reselect": "4.0.0",
    "rudder-sdk-js": "1.0.6",
    "serialize-error": "6.0.0",
    "shallow-equals": "1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check && npm run tsc"
    }
  },
  "jest-junit": {
    "output": "build/test-results.xml"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@types/jest": "25.2.1",
    "@types/moment-timezone": "0.5.13",
    "@types/node": "13.11.0",
    "@types/shallow-equals": "1.0.0",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-mattermost": "github:mattermost/eslint-plugin-mattermost#070ce792d105482ffb2b27cfc0b7e78b3d20acee",
    "husky": "4.2.5",
    "jest": "25.2.7",
    "jest-junit": "10.0.0",
    "mock-socket": "9.0.3",
    "nock": "12.0.3",
    "redux-mock-store": "1.5.4",
    "source-map-support": "0.5.19",
    "ts-jest": "25.3.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
}
